	file = {IEEE Xplore Abstract Record:/Users/milk/Zotero/storage/BGEZ7SPI/1628877.html:text/html;IEEE Xplore Full Text PDF:/Users/milk/Zotero/storage/JE8RILEN/Meijering et al. - 2006 - Tracking in molecular bioimaging.pdf:application/pdf}
}

