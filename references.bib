	langid = {english},
	keywords = {Feature point tracking, Moment scaling spectrum, {READ}, Single particle tracking, {SPT}, Tracker, Trajectory analysis},
	file = {ScienceDirect Full Text PDF:/Users/milk/Zotero/storage/9T95CT53/Sbalzarini and Koumoutsakos - 2005 - Feature point tracking and trajectory analysis for.pdf:application/pdf}
