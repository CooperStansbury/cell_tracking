	keywords = {dynamics, kinetic modeling, single molecule, single particle tracking, super-resolution, transcription factor},
	file = {Full Text PDF:/Users/milk/Zotero/storage/GFZPD9CS/Hansen et al. - 2018 - Robust model-based analysis of single-particle tra.pdf:application/pdf}
}

@article{kowalekClassificationDiffusionModes2019,
